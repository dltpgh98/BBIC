<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/pro_list_bg"
        android:layout_width="230sp"
        android:layout_height="70sp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@color/white"
        android:elevation="5sp"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/pro_list_title_tv"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="5sp"
            android:text="약속 제목"
            android:maxLines="1"
            android:ellipsize="end"
            android:fontFamily="@font/nanum_square_round_b"
            android:textSize="15dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/pro_list_address_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="주소"
            android:maxLines="1"
            android:fontFamily="@font/nanum_square_round_b"
            android:textSize="10sp"
            android:layout_marginStart="5dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/pro_list_title_tv" />

        <ImageView
            android:id="@+id/pro_list_profile1_iv"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/pro_list_address_tv" />
        <ImageView
            android:id="@+id/pro_list_profile2_iv"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            app:layout_constraintStart_toEndOf="@id/pro_list_profile1_iv"
            app:layout_constraintTop_toBottomOf="@id/pro_list_address_tv" />
        <ImageView
            android:id="@+id/pro_list_profile3_iv"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            app:layout_constraintStart_toEndOf="@id/pro_list_profile2_iv"
            app:layout_constraintTop_toBottomOf="@id/pro_list_address_tv" />

        <TextView
            android:id="@+id/pro_list_settime_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="22.03.02 00:00:00"
            android:layout_marginEnd="2sp"
            android:layout_marginBottom="2sp"
            android:textSize="10sp"
            android:maxLines="1"
            android:fontFamily="@font/nanum_square_round_b"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>