<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/subway_top_ly"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:background="@drawable/layout_radious"
        android:backgroundTint="@color/teal_200"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/subway_this_station_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="지하철 당역명"
            android:textSize="25dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />
        <ImageButton
            android:id="@+id/subway_bookmark_ibtn"
            android:layout_width="40dp"
            android:layout_height="30dp"
            android:src="@drawable/ic_baseline_star_24"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <TextView
            android:id="@+id/subway_left_station_direction_image_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="aa"
            app:layout_constraintTop_toBottomOf="@id/subway_this_station_name_tv"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:id="@+id/subway_left_station_direction_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="왼쪽방면"
            app:layout_constraintTop_toBottomOf="@id/subway_this_station_name_tv"
            app:layout_constraintStart_toEndOf="@id/subway_left_station_direction_image_tv"
            />

        <TextView
            android:id="@+id/subway_right_station_direction_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="오른쪽방면"
            app:layout_constraintTop_toBottomOf="@id/subway_this_station_name_tv"
            app:layout_constraintEnd_toStartOf="@id/subway_right_station_direction_image_tv"/>
        <TextView
            android:id="@+id/subway_right_station_direction_image_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="aa"
            app:layout_constraintTop_toBottomOf="@id/subway_this_station_name_tv"
            app:layout_constraintEnd_toEndOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/subway_bottom_left_ly"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/subway_top_ly">

        <TextView
            android:id="@+id/subway_left_station1_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="왼쪽역명"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/subway_left_station1_time_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="도착 시간"
            android:layout_marginStart="5dp"
            app:layout_constraintStart_toEndOf="@id/subway_left_station1_name_tv"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/subway_left_station1_whole_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="(몇전역)"
            android:layout_marginStart="5dp"
            app:layout_constraintStart_toEndOf="@id/subway_left_station1_time_tv"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/subway_left_station2_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="왼쪽역명"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/subway_left_station1_name_tv" />

        <TextView
            android:id="@+id/subway_left_station2_time_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="도착 시간 2"
            android:layout_marginStart="5dp"
            app:layout_constraintStart_toEndOf="@id/subway_left_station2_name_tv"
            app:layout_constraintTop_toBottomOf="@id/subway_left_station1_name_tv" />

        <TextView
            android:id="@+id/subway_left_station2_whole_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="(몇전역)2"
            android:layout_marginStart="5dp"
            app:layout_constraintStart_toEndOf="@id/subway_left_station2_time_tv"
            app:layout_constraintTop_toBottomOf="@id/subway_left_station1_name_tv" />


        <TextView
            android:id="@+id/subway_left_station3_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="왼쪽역명"

            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/subway_left_station2_name_tv" />

        <TextView
            android:id="@+id/subway_left_station3_time_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="도착 시간 3"
            android:layout_marginStart="5dp"
            app:layout_constraintStart_toEndOf="@id/subway_left_station3_name_tv"
            app:layout_constraintTop_toBottomOf="@id/subway_left_station2_name_tv" />

        <TextView
            android:id="@+id/subway_left_station3_whole_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="(몇전역)3"
            android:layout_marginStart="5dp"
            app:layout_constraintStart_toEndOf="@id/subway_left_station3_time_tv"
            app:layout_constraintTop_toBottomOf="@id/subway_left_station2_name_tv" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <View
        android:background="#000000"
        android:layout_width="1dp"
        android:layout_height="50dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintTop_toBottomOf="@id/subway_top_ly"
        app:layout_constraintStart_toEndOf="@id/subway_bottom_left_ly"
        app:layout_constraintEnd_toStartOf="@id/subway_bottom_right_ly"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/subway_bottom_right_ly"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/subway_top_ly">

        <TextView
            android:id="@+id/subway_right_station1_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="왼쪽역명"
            android:layout_marginEnd="5dp"
            app:layout_constraintEnd_toStartOf="@id/subway_right_station1_time_tv"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/subway_right_station1_time_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="도착 시간"
            android:layout_marginEnd="5dp"
            app:layout_constraintEnd_toStartOf="@id/subway_right_station2_whole_tv"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/subway_right_station1_whole_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="(몇전역)1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/subway_right_station2_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="왼쪽역명"
            android:layout_marginEnd="5dp"
            app:layout_constraintEnd_toStartOf="@id/subway_right_station2_time_tv"
            app:layout_constraintTop_toBottomOf="@id/subway_right_station1_name_tv" />

        <TextView
            android:id="@+id/subway_right_station2_time_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="도착 시간 2"
            android:layout_marginEnd="5dp"
            app:layout_constraintEnd_toStartOf="@id/subway_right_station2_whole_tv"
            app:layout_constraintTop_toBottomOf="@id/subway_right_station1_name_tv" />

        <TextView
            android:id="@+id/subway_right_station2_whole_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="(몇전역)2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/subway_right_station1_name_tv" />


        <TextView
            android:id="@+id/subway_right_station3_name_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="왼쪽역명"
            android:layout_marginEnd="5dp"
            app:layout_constraintEnd_toStartOf="@id/subway_right_station3_time_tv"
            app:layout_constraintTop_toBottomOf="@id/subway_right_station2_name_tv" />

        <TextView
            android:id="@+id/subway_right_station3_time_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="도착 시간 3"
            android:layout_marginEnd="5dp"
            app:layout_constraintEnd_toStartOf="@id/subway_right_station3_whole_tv"
            app:layout_constraintTop_toBottomOf="@id/subway_right_station2_name_tv" />

        <TextView
            android:id="@+id/subway_right_station3_whole_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="(몇전역)3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@id/subway_right_station2_name_tv" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>